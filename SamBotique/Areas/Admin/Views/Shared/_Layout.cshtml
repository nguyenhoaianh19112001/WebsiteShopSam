        <!-- /.content-wrapper -->
        <!-- jQuery 3 -->
        <script src="~/Areas/Admin/Luutru/bower_components/jquery/dist/jquery.min.js"></script>
        <!-- jQuery UI 1.11.4 -->
        <script src="~/Areas/Admin/Luutru/bower_components/jquery-ui/jquery-ui.min.js"></script>
        <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
        <script>$.widget.bridge('uibutton', $.ui.button);</script>
        <!-- Bootstrap 3.3.7 -->
        <script src="~/Areas/Admin/Luutru/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
        <!-- Morris.js charts -->
        <script src="~/Areas/Admin/Luutru/bower_components/raphael/raphael.min.js"></script>
